{
  "batch_size": 32,
  "cache_dataset": true,
  "num_workers": 0,
  "pad_with_last_sample": true,
  "train_rate": 0.6,
  "eval_rate": 0.2,
  "scaler": "standard",
  "load_external": true,
  "normal_external": false,
  "ext_scaler": "none",
  "input_window": 12,
  "output_window": 12,
  "add_time_in_day": true,
  "add_day_in_week": true,
  "shuffle": true,

  "hop": 2,
  "num_neighbors": 4,
  "d_model": 128,
  "hidden": 64,
  "dropout_rate": 0.0,
  "act": "relu",
  "num_patches": [256, 32],
  "pool_size": [32, 4],
  "n_slayer": 2,
  "n_layer": 1,
  "n_tlayer": 4,
  "t_d_model": 128,
  "skip_dim": 128,
  "end_dim": 256,
  "bias": false,
  "set_loss": "masked_mae",
  "feat_mix": false,
  "mixer": "pool",


  "seed": 0,
  "max_epoch": 300,
  "learner": "adamw",
  "learning_rate": 1e-3,
  "weight_decay": 0.003,
  "lr_decay": true,
  "lr_scheduler": "multisteplr",
  "steps": [10, 30, 45, 70],
  "lr_eta_min": 1e-5,
  "lr_decay_ratio": 0.5,
  "lr_warmup_epoch": 5,
  "lr_warmup_init": 1e-7,
  "clip_grad_norm": true,
  "max_grad_norm": 5,
  "use_early_stop": true,
  "patience": 20,
  "step_size": 1314,
  "task_level": 0,
  "use_curriculum_learning": true
}
