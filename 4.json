{
  "batch_size": 32,
  "cache_dataset": true,
  "num_workers": 0,
  "pad_with_last_sample": true,
  "train_rate": 0.6,
  "eval_rate": 0.2,
  "scaler": "standard",
  "load_external": true,
  "normal_external": false,
  "ext_scaler": "none",
  "input_window": 12,
  "output_window": 12,
  "add_time_in_day": true,
  "add_day_in_week": true,

  "hop": 2,
  "num_neighbors": 4,
  "d_model": 32,
  "hidden": 32,
  "dropout_rate": 0.1,
  "act": "gelu",
  "num_patches": 32,
  "pool_size": 16,
  "n_layer": 10,
  "n_tlayer": 4,
  "bias": false,


  "seed": 0,
  "max_epoch": 300,
  "learner": "adamw",
  "learning_rate": 1e-4,
  "weight_decay": 0.05,
  "lr_decay": true,
  "lr_scheduler": "cosinelr",
  "lr_eta_min": 1e-5,
  "lr_decay_ratio": 0.05,
  "lr_warmup_epoch": 5,
  "lr_warmup_init": 1e-7,
  "clip_grad_norm": true,
  "max_grad_norm": 5,
  "use_early_stop": true,
  "patience": 50,
  "step_size": 670,
  "task_level": 0,
  "use_curriculum_learning": true
}
